// cstddef standard header
#ifndef _CSTDDEF_
#define _CSTDDEF_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <stddef.h>
 #define _STD_USING

#else /* _STD_USING */
 #include <stddef.h>

 #ifdef _GLOBAL_USING
_STD_BEGIN
using _CSTD ptrdiff_t; using _CSTD size_t;
_STD_END
 #endif /* _GLOBAL_USING */

#endif /* _STD_USING */
#endif /* _CSTDDEF_ */

